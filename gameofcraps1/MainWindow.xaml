<Window x:Class="gameofcraps1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:gameofcraps1"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="370">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="30,42,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="23" Margin="30,123,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="141"/>
        <TextBox x:Name="textBox2" HorizontalAlignment="Left" Height="23" Margin="142,188,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="195"/>
        <Label x:Name="label" Content="Result" HorizontalAlignment="Left" Margin="80,188,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button" Content="Play" HorizontalAlignment="Left" Margin="262,42,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Button x:Name="button1" Content="Roll" HorizontalAlignment="Left" Margin="262,122,0,0" VerticalAlignment="Top" Width="75" IsEnabled="False" Click="button1_Click"/>

    </Grid>
</Window>
